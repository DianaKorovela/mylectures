package lv.diana;

public class LearnMethods {

	/**
	 * @param args
	 */
	public static void main(String[] args) { // void key word not return type // static key word // 
		// public private protected 
		
	}
	
	
	public static void getMyName() {
		System.out.println("My name is Diana");
	}

	public static void getMyCity("I'm from Riga") {
	}

	public static void getIhaveDog() {
	}

}
