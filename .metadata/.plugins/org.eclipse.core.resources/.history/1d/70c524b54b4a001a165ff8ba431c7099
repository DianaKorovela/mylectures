import java.util.ArrayList;


public class TestTwoArrays {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		int [] arr1 = {2, 3, 5, 8};
		int [] arr2 = {2, 7, 5};
		
		ArrayList<Integer> arrList = new ArrayList<>();

		for(int i : arr1){
			for(int j : arr2){
				if(i == j){
					arrList.add(i); 
				}
			}
		}
	}

}
