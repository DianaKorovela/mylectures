
public class TaskVar {

	/**
	 * @param args
	 * 125 + 24 = 149
	   125 - 24 = 101
       125 x 24 = 3000
       125 / 24 = 5
       125 mod 24 = 5
	 */
	public static void main(String[] args) { 
		int num = 125; 
		int num1 = 24; 
		
		
		int numAdd = 125 + 24;
		ing numMinus = 125 - 24; 
		
		 
		System.out.println(numAdd + " = 125 + 24");
		System.out.println(num + num1);
		
	}

}
