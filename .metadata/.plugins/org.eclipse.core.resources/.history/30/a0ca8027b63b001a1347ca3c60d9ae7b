
public class TestVersion {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		int[] marks = {76, 23, 42, 58, 35, 85};
		// For-Each Loops for(type var : arrays
		
		for(int mark : marks){
		for (int i = 0; i < marks.length; i++) {
				System.out.println(marks[i]);
			System.out.println("++++++++++++++++++++++++++++++");	
				System.out.println(mark);
		}
		}
}
}