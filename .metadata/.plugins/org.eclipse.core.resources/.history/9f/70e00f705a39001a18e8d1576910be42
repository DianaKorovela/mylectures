
public class TaskVar {

	/**
	 * @param args
	 * 
	 */
	public static void main(String[] args) { 
		int num = 125; 
		int num1 = 24; 
		
		int numAdd = num + num1;
		int numMinus = 125 - 24; 
		int numDivide = 125 / 24; 
	
		System.out.println(numAdd + " = 125 + 24");
		System.out.println(numMinus + " = 125 - 24");
		System.out.println(num + " = 125 / 24");
		System.out.println(numAdd + " = 125 * 24");
		System.out.println(numAdd + " = 125 % 24");
		
		System.out.println(num + num1);
		System.out.println(num - num1);
		System.out.println(num / num1);
		System.out.println(num * num1);
		System.out.println(num % num1);
		
		
	}

}
