
public class PrimType {

	
	public static void main(String[] args) {
		// Numeric data primitive type
		int integer  = 2; 
		long longInter = 2020202020202L; 
		short shortInteger = 2020;
		byte byteInteger = 120; 
		
		// Floating-Point type real numbers
		double doubleFloatNumber = 0.5d;
		float floatNumber = 0.11181811818f;
		
		// Characters 
		char charSymbol = 'a';
		char charSymbolAsNumber = 33;
		
		System.out.println("Char: "+charSymbolAsNumber);
		
		char D = 68;
		char I = 73;
		char A = 65;
		
		System.out.println(charSymbolAsNumber);
		System.out.println("" +D+I+A);
		
		//Booleans 
		boolean booleanTrueFalse = false; 
		boolean booleanValueCheck;
		System.out.println("Char: "+charSymbolAsNumber);
		System.out.println(booleanTrueFalse);
		System.out.println(booleanValueCheck);
		
				

	}

}
