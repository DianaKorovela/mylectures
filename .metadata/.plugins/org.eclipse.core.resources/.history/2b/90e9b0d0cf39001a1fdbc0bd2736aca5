
public class PrintNumberInWord {

	
	public static void main(String[] args) {
		
		int number = 1;
		int number2 = 2;
		int number3 = 3;
		

		if(number == 1){
			System.out.println("One");
		}else if (number == 2){
			System.out.println("Two");	
	}else{
		System.out.println("Good evening");
	}

	}


	}

